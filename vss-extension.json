{
    "manifestVersion": 1,
    "id": "yavt",
    "name": "YAVT - Yet another versioning tool",
    "version": "1.5.1",
    "publisher": "tschmiedlechner",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Yet another versioning tool - Git versioning, made simple.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "yavt",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/tschmiedlechner/yavt"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "galleryFlags": [
        "Public"
    ]
}